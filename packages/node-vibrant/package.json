{
  "name": "node-vibrant",
  "version": "3.0.0",
  "description": "Extract prominent colors from an image. Supports both node and browser environment.",
  "main": "lib/index.js",
  "browser": "lib/bundle-worker.js",
  "types": "lib/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@vibrant/core": "^3.0.0",
    "@vibrant/generator-default": "^3.0.0",
    "@vibrant/image-browser": "^3.0.0",
    "@vibrant/image-node": "^3.0.0",
    "@vibrant/quantizer-mmcq": "^3.0.0",
    "lodash": "^4.17.11",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "karma": "^3.1.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-commonjs": "^1.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.5",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.3.2",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "clean": "npm run clean:browser && npm run clean:node",
    "clean:browser": "rimraf dist",
    "clean:node": "rimraf lib",
    "build": "npm run build:browser && npm run build:node",
    "build:browser": "webpack --config webpack.config.js",
    "build:node": "tsc",
    "pretest": "npm run clean && npm run build",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "mocha lib/test/**/*.spec.js",
    "test:browser": "karma start karma.conf.js",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akfish/node-vibrant.git"
  },
  "keywords": [
    "color",
    "detection",
    "varation",
    "image",
    "picture",
    "canvas",
    "vibrant",
    "muted",
    "colour"
  ],
  "author": "akfish",
  "license": "MIT"
}
